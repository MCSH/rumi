
printf := (T: string, s: ...any)->void;

add_one_to_array:=(A: int[], count: int)->void{

  printf("Inside add_one_to_array(a, 2)\n");

  while(count){
    count = count - 1;
    A[count] = A[count] + 1;
  }

  return;
}

main := ()->int{
  a : int[2];
  a[0] = 1;
  a[1] = 2;

  printf("a = [%d, %d]\n", a[0], a[1]);

  add_one_to_array(a, 2);

  printf("a = [%d, %d]\n", a[0], a[1]);

  d: int[];
  d = a;
  d[0] = 12;
  d[1] = 3;

  printf("a = [%d, %d]\n", a[0], a[1]);
  printf("d = [%d, %d]\n", d[0], d[1]);

  return 0;
}
