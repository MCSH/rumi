

printf := (t: string, f: ...any) -> int;

main := ()->int{
  arr : int[10];
  x := 3;

  arr[2] = 1;

  a := 2;

  arr[a] = 4;

  printf("the array's 3rd member was %d\n", arr[a]);

  arr2 : int[x];

  arr2[0] = 3;

  printf("the second array's 1st member was %d\n", arr2[0]);

  return 0;

