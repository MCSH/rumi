printInt := (i: int) -> unit;
printString := (s: string) -> unit;
malloc := (i: int) -> *any;
free := (i: *any) -> unit;
printf := (f: string, a: ...any)->unit;

TestS: struct{
  id: int;
}

TestS.print := (a: int) -> unit{
  printf("<TestS id=%d , a = %d>\n", self.id, a);
  //printf("Inside TestS Print!\n");
  return ;
}

TestS.getId := () -> int{
  return self.id;
}

Printable: interface{
  print := (a: int) -> unit;
}

main := () -> int {

  i: Printable;
  ip: *Printable = malloc(sizeof Printable);

  t : *TestS = malloc(sizeof TestS);
  //t : TestS;

  i = t -> Printable;
  *ip = i;

  t.id = 10;

  t.print(0);
  tmp := t.getId();

  t2 := t.id;

  printf("t.getId() = %d\nt.id=%d\n", tmp, t2);

  i.print(90);
  ip.print(100);

  return 0;
}